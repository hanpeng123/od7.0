<?xml version="1.0" encoding="utf-8"?>
<openerp>
	<data>

		<record model="ir.ui.view" id="inv_view_form">
			<field name="name">stu.form.view</field>
			<field name="model">hp.inv</field>
			<field name="priority" eval="10"/>
			<field name="arch" type="xml">
				<form string="" version="7.0">
					<sheet>
						<group>
							<field name="code" />
							<field name="name" />
							<field name="state"/>
						</group>
					</sheet>
				</form>
			</field>
		</record>
		
		<record model="ir.ui.view" id="inv_view_form2">
			<field name="name">stu.form.view</field>
			<field name="model">hp.inv</field>
			<field name="priority" eval="12"/>
			<field name="arch" type="xml">
				<form string="" version="7.0">
					<sheet>
						<group>
							<field name="code" />
							<field name="name" />
							<field name="state" />
						</group>
					</sheet>
				</form>
			</field>
		</record>

		<record model="ir.ui.view" id="inv_view_tree">
			<field name="name">stu.tree.view</field>
			<field name="model">hp.inv</field>
			<field name="arch" type="xml">
				<tree string="" version="7.0">
					<field name="code" nolabel="1"/>
					<field name="name" />
					<field name="address" />
					<field name="state" />
				</tree>
			</field>
		</record>

		<record model="ir.actions.act_window" id="inv_acitons">
			<field name="name">students</field>
			<field name="res_model">hp.inv</field>
			<field name="view_type">form</field>
			<field name="view_mode">tree,form</field>
			<field name="view_id" ref="stu_view_tree" />
		</record>


		<menuitem id="create_students_menu" name="students"
			parent="test_express_use_menu" action="inv_acitons" />

	</data>
</openerp>